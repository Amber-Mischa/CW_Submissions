public class Intro_Cube {

	public static void main(String[] args) {
		/* Calculate the surface area and volume of a cube 
		 * with a side of 5.
	 	 * 
	  	 * Var Key
		 * s means side length
		 * sura means surface area
		 * v means volume
		 */
		double s = 5;
		//Find the surface area of the cube
		double sura = 6*s;
		//Find the volume of the cube
		double v = s*s*s;	
		//Print volume and surface area as ints
		System.out.print("\nThis is the volume of the Cube as an int " + (int)v + ".");
		System.out.print("This is the surface are of the Cube as an int " + (int)sura + ".");
	}

}
